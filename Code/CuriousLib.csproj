<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<AssemblyName>CuriousLib</AssemblyName>
    <Description>C# Class Library for Statistical Computations</Description>
	<Version>1.3</Version>
    <Authors>Ali Özgür Dede</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/AliOzgurDede/CuriousLib</RepositoryUrl>
    <RepositoryType>Class Library</RepositoryType>
    <PackageTags>Statistics</PackageTags>
    <PackageProjectUrl>https://github.com/AliOzgurDede/CuriousLib</PackageProjectUrl>
    <ApplicationIcon>185852.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.OleDb" Version="6.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
  </ItemGroup>

</Project>
